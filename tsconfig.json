{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "./src",
    "paths": {
      "@Components": [
        "components"
      ],
      "@Components/*": [
        "components/*"
      ],
      "@API": [
        "apis"
      ],
      "@API/*": [
        "apis/*"
      ],
      "@Fonts": [
        "assets/fonts"
      ],
      "@Fonts/*": [
        "assets/fonts/*"
      ],
      "@Images": [
        "assets/images"
      ],
      "@Images/*": [
        "assets/images/*"
      ],
      "@Utils": [
        "utils"
      ],
      "@Utils/*": [
        "utils/*"
      ],
      "@Types": [
        "types"
      ],
      "@Types/*": [
        "types/*"
      ],
      "@Misc": [
        "misc"
      ],
      "@Misc/*": [
        "misc/*"
      ],
      "@Middleware": [
        "middleware"
      ],
      "@Middleware/*": [
        "middleware/*"
      ],
      "@Pages": [
        "pages"
      ],
      "@Pages/*": [
        "pages/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx",
    "types": [
      "node",
      "react",
      "styled-components",
      "jest",
      "chrome"
    ]
  },
  "include": [
    "src/**/*",
    "craco.config.js"
  ]
}